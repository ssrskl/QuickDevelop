    <sql id="selectDqArticlePostProcessorVO">
        select dq_article.article_id,
               dq_article.article_title,
               dq_article.article_content,
               dq_article.article_image,
               dq_article.section_id,
               dq_article.section_type_id,
               dq_article.author_id,
               dq_article.status,
               dq_article.article_weight,
               dq_article.create_time,
               dq_article.update_time,
               ds.section_id,
               ds.section_name,
               ds.section_introduce,
               ds.section_logo,
               ds.section_background,
               ds.section_admin_user_id,
               ds.section_weight,
               ds.create_time,
               ds.update_time,
               dst.section_type_id,
               dst.section_type_name,
               dst.section_id,
               dst.section_type_weight,
               dst.section_type_mold,
               dst.section_type_network,
               dst.create_time,
               dst.update_time,
               du.user_id,
               du.user_name,
               du.email,
               du.phone_number,
               du.sex,
               du.avatar,
               du.status,
               du.loginIp,
               du.loginDate,
               du.signature,
               du.grade,
               du.experience,
               du.check_param,
               du.check_status,
               du.school_id,
               du.create_time,
               du.update_time,
               dur.user_role_id,
               dur.role_name,
               dur.user_id,
               dur.role_status,
               dur.create_time,
               dur.update_time
        from dq_article
                 left join dq_section ds on dq_article.section_id = ds.section_id
                 left join dq_section_type dst on dq_article.section_type_id = dst.section_type_id
                 left join dq_user du on dq_article.author_id = du.user_id
                 left join dq_user_role dur on du.user_id = dur.user_id
    </sql>
